#include "F28x_Project.h"



// ----------------------------------------------------------------------
// 全局变量
// ----------------------------------------------------------------------

// 主函数入口
int main(void)
{

    InitSysCtrl();
    InitBoard();


    while(1)
    {
    GpioDataRegs.GPBTOGGLE.bit.GPIO34=1;
    GpioDataRegs.GPATOGGLE.bit.GPIO23=1;
    DELAY_US(1000000);

    }

}


